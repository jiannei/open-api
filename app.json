{
    "addons": [
        "heroku-postgresql",
        "heroku-redis",
        "scheduler"
    ],
    "env": {
        "APP_NAME": "sinan-open-api",
        "APP_ENV": "review",
        "APP_KEY": {
            "generator": "secret"
        },
        "APP_DEBUG": "true",
        "LOG_CHANNEL": "errorlog",
        "DB_CONNECTION": "pgsql",
        "CACHE_DRIVER": "redis",
        "QUEUE_CONNECTION": "redis",
        "SESSION_DRIVER": "redis"
    }
}
